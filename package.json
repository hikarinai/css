{
  "name": "tenoxui-workspace",
  "version": "1.0.0",
  "description": "TenoxUI framework workspace.",
  "repository": "https://github.com/tenoxui/tenoxui",
  "author": "NOuSantx <nousantx@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "publish:tenoxui": "./script/workspace-tenox.js run tenoxui prepublish",
    "wst:build": "mkdir script/.temp && uglifyjs ./script/workspace-tenox.js -c -m toplevel  -o ./script/.temp/wst.temp.js && bash ./script/wst-build.sh && rm -rf script/.temp",
    "wst:start": "./script/workspace-tenox.js interactive"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "uglify-js": "^3.19.3"
  }
}
